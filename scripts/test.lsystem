
brush soft_yellow {

    size: 3,
    color: #ffeda0,
    speed: 500

};

col c = #ffa585;

canvas orange {

    high: 4000,
    width: 4000,
    color: c
};


lsys leaf {
    axiom : A,
    F => >F<,
    A => F[+X]FB,
    B => F[-Y]FA,
    X => A,
    Y => B
};

bool b = true;


if (b) {
    draw(leaf, soft_yellow, orange, 5, 45, 10);
};