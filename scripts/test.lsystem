brush mibrocha {

    size: 1,
    color: #c93c20,
    speed: 500

};

canvas micanvas {

    high: 4000,
    width: 4000,
    color: #ffffff
};


lsys leaf {
    axiom : A,
    F => >F<,
    A => F[+X]FB,
    B => F[-Y]FA,
    X => A,
    Y => B
};

string a = HOLA;

if (a == HOLA) {
    draw(leaf, mibrocha, micanvas, 5 ,45, 8);
};