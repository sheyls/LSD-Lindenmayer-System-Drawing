_int a = 3;
a = 5;


lsys star{
    axiom : F++F-F++F-F++F-F++F-F++F-F++F,
    F => F+F--F+F--F+F--F+F--F+F--F+F--FFF
};

lsys leaf {
    axiom : A,
    F => >F<,
    A => F[+X]FB,
    B => F[-Y]FA,
    X => A,
    Y => B
};

lsys square_sierpinski{
    axiom : F+XF+F+XF,
    X => XF-F+F-XF+F+XF-F+F-X%$
};

lsys bush {
    axiom : F,
    F => F[-F]F[+F][F]
};


brush mibrochamucha {

    size: 1,
    color: #c93c20,
    speed: 500

};

canvas micanvasgambas {

    high: 4000,
    width: 4000,
    color: #ffffff
};
lsys hexagonal_gosper{
    axiom : XF,
    X => X+YF++YF-FX--FXFX-YF+,
    Y => -FX+YFYF++YF+FX--FX-Y
};


draw(leaf, mibrochamucha, micanvasgambas, 5, 45, 10);