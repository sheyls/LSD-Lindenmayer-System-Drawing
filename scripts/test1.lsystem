lsys quedratic_snowflake{
    axiom : F,
    F => F-F+F+F-F

};
 
int angle = 90;

brush small_blue {

    size: 2,
    color: #40e0d0,
    speed: 300

};

canvas soft_orange {

    high: 4000,
    width: 4000,
    color: #ffebcd
};

int a = 5;

repeat 5 {
    if (a (<) 15) {
        draw(quedratic_snowflake, small_blue, soft_orange, a, angle, 3);
        angle = angle (+) 10;
    };
};
