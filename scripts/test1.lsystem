_int a = 3;
a = 5;

_int test = 60;

lsys star{
    axiom : F++F-F++F-F++F-F++F-F++F-F++F,
    F => F+F--F+F--F+F--F+F--F+F--F+F--FFF
};



lsys square_sierpinski{
    axiom : F+XF+F+XF,
    X => XF-F+F-XF+F+XF-F+F-X%$
};

lsys bush {
    axiom : F,
    F => FF+[+F-F-F]-[-F+F+F]
};





repeat 2 {
    lsys hexagonal_gosper{
    axiom : XF,
    X => X+YF++YF-FX--FXFX-YF+,
    Y => -FX+YFYF++YF+FX--FX-Y
};

lsys leaf {
    axiom : A,
    F => >F<,
    A => F[+X]FB,
    B => F[-Y]FA,
    X => A,
    Y => B
};
brush mibrochamucha {

    size: 1,
    color: #c93c20,
    speed: 500

};

canvas micanvasgambas {

    high: 4000,
    width: 4000,
    color: #ffffff
};
    

draw(leaf, mibrochamucha, micanvasgambas, 5, 45, 8);

lsys star{
    axiom : F++F-F++F-F++F-F++F-F++F-F++F,
    F => F+F--F+F--F+F--F+F--F+F--F+F--FFF
};

brush mibrocha {

    size: 1,
    color: #c93c20,
    speed: 500

};

canvas micanvas {

    high: 4000,
    width: 4000,
    color: #ffffff
};

repeat 2 {

    draw(leaf, mibrocha, micanvas, 5 ,60, 8);
    a = 10;
    test = 70;
    brush mibrocha {

        size: 1,
        color: #808080,
        speed: 500

    };

};

if (true) {
    draw(leaf, mibrocha, micanvas, 5 ,90, 8);
};