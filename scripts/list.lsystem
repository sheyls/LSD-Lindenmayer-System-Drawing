
lsys leaf {
    axiom : A,
    F => >F<,
    A => F[+X]FB,
    B => F[-Y]FA,
    X => A,
    Y => B
};

brush mibrochamucha {

    size: 2,
    color: #ffffff,
    speed: 300

};

int k = 4000;
canvas micanvasgambas {

    high: 4000,
    width: 4000,
    color: #000000
};

change_axiom( leaf, AC );
add_rule(leaf, C => &+F#F );
draw(leaf, mibrochamucha, micanvasgambas, 5, 35, 5);

