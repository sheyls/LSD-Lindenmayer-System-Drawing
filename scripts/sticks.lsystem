#Catalog

lsys stick {
    axiom : X,
    F => FF,
    X => F[+X]F[-X]+X
};
#stick_angle = 20

lsys weed {
    axiom : F,
    F => FF-[XY]+[XY],
    X => +FY,
    Y => -FX
};
#weed_angle = 22.5

#Drawing example

