#Catalog

lsys stick {
    axiom : X,
    F => FF,
    X => F[+X]F[-X]+X
};
#stick_angle = 20

lsys weed {
    axiom : F,
    F => FF-[XY]+[XY],
    X => +FY,
    Y => -FX
};
#weed_angle = 22.5

#Drawing example

brush mibrocha {

    size: 2,
    color: #ffffff,
    speed: 300

};

canvas micanvas {

    high: 4000,
    width: 4000,
    color: #000000
};

draw(stick, mibrocha, micanvas, 5 ,20, 8);

